entrypoint="index.html"
hidden=[".config"]
modules = ["nodejs-20", "web:v2-20230623-0b7a606"]

[nix]
channel = "stable-23_11"

[deployment]
publicDir = "/"
deploymentTarget = "static"
run = ["sh", "-c", "python3 -m http.server 3000 --bind 0.0.0.0"]

[workflows]
runButton = "Run"

[[workflows.workflow]]
name = "Run"
author = 25742947

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx serve -l 3000"

[[ports]]
localPort = 3000
externalPort = 80
